<!-- 最好加入一个面包屑导航 -->
<view class="top_box">
  <van-row>
    此处应该添加一个面包屑导航&返回
  </van-row>
  <van-row>
  <van-col span="12">
        <van-dropdown-menu style="width:100%">
          <van-dropdown-item value="{{ value1 }}" options="{{ option1 }}" />
        </van-dropdown-menu >
  </van-col>
  <van-col span="12">
    <van-search style="width:100%" value="{{ value }}" placeholder="请输入搜索关键词" />
  </van-col>
</van-row>
</view>
<text>\n\n\n\n</text>
<view>
<van-cell-group wx:for="{{fileList}}">
  <van-cell wx:if="{{item.type == 0}}" title="{{item.name}}"  bindlongpress="popUp" is-link />
  <van-cell wx:else title="{{item.name}}"  clickable="true" bindlongpress="popUp" bindtap="openObj"
  data-item="{{item}}"/>
</van-cell-group>
</view>

<van-popup show="{{ popShow }}"  position="bottom"   custom-style="height: 30%;"
  bind:close="popDown">
  <van-cell  title="删除" clickable="true" bindtap="delObj"  />
  <van-cell  title="复制"  clickable="true" bindtap="copyObj"/>
  <van-cell  title="移动"  clickable="true" bindtap="moveObj"/>
</van-popup>


